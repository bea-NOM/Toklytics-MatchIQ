{
  "name": "toklytics-matchiq",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "20.x || 22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start -p 3000",
    "type-check": "prisma generate && tsc --noEmit",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "ts-node --compiler-options '{\"module\":\"commonjs\"}' prisma/seed.ts",
    "cron": "ts-node --compiler-options '{\"module\":\"commonjs\"}' src/jobs/cron.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prisma:resolve": "bash -lc 'set -a; source .env; set +a; pnpm prisma migrate resolve --applied 0000_baseline'",
    "prisma:migrate": "bash -lc 'set -a; source .env; set +a; pnpm prisma migrate dev -n continue_after_baseline'",
    "prisma:generate": "bash -lc 'set -a; source .env; set +a; pnpm prisma generate'",
    "prisma:seed": "bash -lc 'set -a; source .env; set +a; pnpm prisma db seed'",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@prisma/extension-accelerate": "^2.0.2",
    "@prisma/extension-optimize": "^2.0.0",
    "@supabase/ssr": "0.7.0",
    "@supabase/supabase-js": "^2.45.0",
    "@tanstack/react-table": "^8.11.8",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.441.0",
  "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "^3.3.0",
  "stripe": "file:vendor/stripe-node",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@prisma/config": "^6.16.3",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "prisma": "^6.16.3",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}

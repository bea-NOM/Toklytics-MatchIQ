# Example environment variables
# Enable GPT-5 preview for clients (set to "true" to enable)
# Note: For extra safety this repository requires both flags to be set:
# - ENABLE_GPT5=true
# - ENABLE_GPT5_ALLOW=true
ENABLE_GPT5=false
ENABLE_GPT5_ALLOW=false

# Example: OpenAI API key (when you integrate with OpenAI)
# OPENAI_API_KEY=sk-xxxxxxxx
# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
DATABASE_URL=

# Stripe
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_ID_PRO=
STRIPE_PRICE_ID_AGENCY=
# or use lookup keys instead of raw price IDs
STRIPE_PRICE_LOOKUP_KEY_PRO=
STRIPE_PRICE_LOOKUP_KEY_AGENCY=
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Resend
RESEND_API_KEY=
RESEND_FROM_EMAIL="Toklytics <no-reply@yourdomain.com>"

# TikTok webhook signature (simulated)
TIKTOK_WEBHOOK_SECRET=devsecret

# Backstage validation (simulated)
BACKSTAGE_SIGNING_SECRET=devsecret

# App
APP_URL=http://localhost:3000
NODE_ENV=development
